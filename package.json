{
  "name": "fns-client",
  "version": "1.0.3",
  "description": "Client for get receipts using the FNS API.",
  "author": "Nikolay Naumenkov",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "fns",
    "api",
    "axios"
  ],
  "scripts": {
    "prebuild": "npm run del",
    "build": "tsc -p tsconfig.build.json",
    "prewatch": "npm run del",
    "watch": "tsc -p tsconfig.build.json --watch",
    "del": "rimraf dist",
    "format": "prettier --write \"{lib,tests}/**/*.ts\"",
    "lint": "eslint \"{lib,tests}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "postversion": "npm run push",
    "push": "git push origin master --tags",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "axios-auth-refresh": "^3.3.3",
    "fns-api": "^1.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-angular": "^17.1.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "axios-mock-adapter": "^1.21.2",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/fns-client.git"
  },
  "bugs": {
    "url": "https://github.com/nikolaynau/fns-client/issues"
  },
  "homepage": "https://github.com/nikolaynau/fns-client#readme"
}
