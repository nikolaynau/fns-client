{
  "name": "fns-client",
  "version": "1.0.5",
  "description": "Client for get receipts using the FNS API.",
  "author": "Nikolay Naumenkov",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "fns",
    "api",
    "axios"
  ],
  "scripts": {
    "prebuild": "npm run del",
    "build": "tsc -p tsconfig.build.json",
    "prewatch": "npm run del",
    "watch": "tsc -p tsconfig.build.json --watch",
    "del": "rimraf dist",
    "format": "prettier --write \"{lib,tests}/**/*.ts\"",
    "lint": "eslint \"{lib,tests}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "postversion": "npm run push",
    "push": "git push origin master --tags",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "axios": ">= 0.27.2"
  },
  "dependencies": {
    "axios-auth-refresh": "^3.3.6",
    "fns-api": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-angular": "^18.4.3",
    "@types/jest": "^29.5.9",
    "@types/node": "^20.9.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "axios": "^1.6.2",
    "axios-mock-adapter": "^1.22.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/fns-client.git"
  },
  "bugs": {
    "url": "https://github.com/nikolaynau/fns-client/issues"
  },
  "homepage": "https://github.com/nikolaynau/fns-client#readme"
}
